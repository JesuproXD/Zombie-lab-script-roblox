local function IBGPQY_fake_script() -- Boton.LocalScript 
	local script = Instance.new('LocalScript', Boton)

	script.Parent.MouseButton1Click:Connect(function()
		workspace.ZombieBlockers.Blocker.BrickColor = BrickColor.new("Mint")
		game.ReplicatedStorage.LocalFolder.AntiWeaponZone:Destroy()
		workspace.AntiWeaponZone:Destroy()
		script.Parent.Parent.Parent.Parent.EjecutadoCorrectamente.BackgroundTransparency = 0.4
		wait(0.1)
		script.Parent.Parent.Parent.Parent.EjecutadoCorrectamente.BackgroundTransparency = 0.9
		while true do
			workspace.ZombieBlockers.Blocker.CanCollide = false
			wait(0.1)
		end
	end)
end
coroutine.wrap(IBGPQY_fake_script)()
local function MQENXKL_fake_script() -- Boton_2.Pistola 
	local script = Instance.new('LocalScript', Boton_2)

	script.Parent.Parent.Parent.Parent.EjecutadoCorrectamente.BackgroundTransparency = 0.4
	wait(0.1)
	script.Parent.Parent.Parent.Parent.EjecutadoCorrectamente.BackgroundTransparency = 0.9
	
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	
	repeat wait() until LocalPlayer.Backpack:FindFirstChild("Pistol") or LocalPlayer.Character:FindFirstChild("Pistol")
	
	local function buffGun()
		local Pistol = LocalPlayer.Backpack:FindFirstChild("Pistol") or LocalPlayer.Character:FindFirstChild("Pistol")
		if not Pistol then return end
	
		for _, v in ipairs(Pistol:GetDescendants()) do
			if v:IsA("ModuleScript") then
				local success, config = pcall(require, v)
				if success and typeof(config) == "table" and config["BaseDamage"] then
					config["BaseDamage"] = 5000
					config["HeadshotDamageMultiplier"] = 5
					config["AmmoPerClip"] = 9999
					config["Clips"] = math.huge
					config["MaxClip"] = math.huge
					config["LimitedClipEnabled"] = false
	
					config["FireRate"] = 0.10
					config["Spread"] = 0
					config["Knockback"] = 220
					config["Lifesteal"] = 1
				end
			end
		end
	end
	
	buffGun()
end
coroutine.wrap(MQENXKL_fake_script)()
