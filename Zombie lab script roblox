local function SNWKQQY_fake_script() -- Boton_7.LocalScript 
	local script = Instance.new('LocalScript', Boton_7)

	local frame = script.Parent.Parent.Parent.Parent.Parent.Parent:WaitForChild("Panel")
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.EjecutadoCorrectamente.BackgroundTransparency = 0.4
		wait(0.1)
		script.Parent.Parent.Parent.Parent.EjecutadoCorrectamente.BackgroundTransparency = 0.9
	
		if frame.ImageTransparency == 1 then
			frame.ImageTransparency = 0.9
		else
			frame.ImageTransparency = 1
		end
	end)
end
coroutine.wrap(SNWKQQY_fake_script)()
