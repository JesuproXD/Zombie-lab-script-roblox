-- Scripts:

local function XZZW_fake_script() -- BotonPrincipal.LocalScript 
	local script = Instance.new('LocalScript', BotonPrincipal)

	local button = script.Parent
	local frame = script.Parent.Parent:WaitForChild("Panel")
	
	button.MouseButton1Click:Connect(function()
		frame.Visible = not frame.Visible
	end)
	
end
coroutine.wrap(XZZW_fake_script)()
local function ASSIDRO_fake_script() -- BotonPrincipal.LocalScript 
	local script = Instance.new('LocalScript', BotonPrincipal)

	local imageButton = script.Parent
	local gameId = 6741970382
	
	-- Construye la URL de la miniatura del juego
	local thumbUrl = "https://www.roblox.com/asset-thumbnail/image?assetId="..gameId.."&width=420&height=420&format=png"
	
	-- Asigna la imagen al ImageButton
	imageButton.Image = thumbUrl
	
end
coroutine.wrap(ASSIDRO_fake_script)()
