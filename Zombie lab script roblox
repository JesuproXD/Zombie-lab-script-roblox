	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	
	repeat wait() until LocalPlayer.Backpack:FindFirstChild("Revolver") or LocalPlayer.Character:FindFirstChild("Revolver")
	
	local function buffGun()
		local Pistol = LocalPlayer.Backpack:FindFirstChild("Revolver") or LocalPlayer.Character:FindFirstChild("Revolver")
		if not Pistol then return end
	
		for _, v in ipairs(Pistol:GetDescendants()) do
			if v:IsA("ModuleScript") then
				local success, config = pcall(require, v)
				if success and typeof(config) == "table" and config["BaseDamage"] then
					config["BaseDamage"] = 5000
					config["HeadshotDamageMultiplier"] = 5
					config["AmmoPerClip"] = 9999
					config["Clips"] = math.huge
					config["MaxClip"] = math.huge
					config["LimitedClipEnabled"] = false
	
					config["FireRate"] = 0.10
					config["Spread"] = 0
					config["Knockback"] = 220
					config["Lifesteal"] = 1
				end
			end
		end
	end
	
	buffGun()
end
